<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.zing.zalo.hsapp">
    <uses-permission android:name="com.android.alarm.permission.SET_ALARM" />
    <application
        android:name=".presentation.MyApplication"
        android:allowBackup="true"
        android:icon="@drawable/img_sleeping"
        android:label="@string/app_name"
        android:roundIcon="@drawable/img_sleeping"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:screenOrientation="portrait"
            android:theme="@style/MainTheme"
            android:name="com.zing.zalo.hsapp.presentation.view.activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <receiver android:name="com.zing.zalo.hsapp.framework.alarm.receiver.AlarmReceiver" />
    </application>

</manifest>